{
  "name": "listrello3",
  "version": "3.0.1",
  "license": "MIT",
  "private": true,
  "author": "Stephen Corbett",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "postinstall": "license-checker --production --json  --excludePackages 'listrello3@3.0.1' > ./src/assets/licences.json",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e --browser chrome'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "axios": "^1.3.2",
    "floating-vue": "^2.0.0-beta.20",
    "is-mobile": "^3.1.1",
    "moment": "^2.29.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.11",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@cypress/vue": "^3.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.6.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "license-checker": "^25.0.1",
    "netlify-plugin-csp-generator": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.15.4",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.0.12"
  },
  "engines": {
    "node": ">=16.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveCorbett/LisTrello"
  }
